import {
  AuthGuardService as AuthGuard
} from '@app/auth/auth.guard.service';
import { Routes } from '@angular/router';
import { <%= modelClass %>ListComponent } from './<%=dasherize(name)%>-list/<%=dasherize(name)%>-list.component';
import { <%= modelClass %>EditComponent } from './<%=dasherize(name)%>-edit/<%=dasherize(name)%>-edit.component';

export const <%=name.toUpperCase()%>_ROUTES: Routes = [
  {
    path: '',
    component: <%= modelClass %>ListComponent,
    canActivate: [AuthGuard]
  },
  {
    path: ':id/edit',
    component: <%= modelClass %>EditComponent,
    canActivate: [AuthGuard]
  },
  {
    path: 'new',
    component: <%= modelClass %>EditComponent,
    canActivate: [AuthGuard]
  }
];
